<%- include('partials/head') %>

    <h1>All Projects</h1>
    <ul>
        <% let allProject=[] %>
            <% for (let task of tasks) { %>
                <% if(!allProject.includes(task.projectName)){ %>
                    <li>
                        <%= task.projectName %>
                    </li>
                    <% allProject.push(task.projectName) %>
                        <% } %>
                            <% } %>

    </ul>

    <%- include('partials/footer') %>